{
    "displayName":"Standard Shapes (2D/3D)",
    "internalName":"stdShape",
    "description":"This model describes \nthe air flow various standard shapes\n either using 2D simulation or a 3D extrusion.",
    "icon":"placeholderIcon.png",
    "debugOnly":true,
    "disable":true,
    "stages":{
        "mesh":{
            "displayName":"Mesh",
            "app":"cwe-serial",
            "groups":[
                "Building Geometry",
                "Mesh Density",
                "Subdomains",
                "Boundaries"
            ],
            "results":[
                {
                    "displayName":"Mesh View",
                    "type":"GLmesh",
                    "showCondition":"XXXX"
                },
                {
                    "displayName":"Visualize 3D Mesh",
                    "type":"GLmesh3D",
                    "showCondition":"XXXX"
                },
                {
                    "displayName":"Mesh Agave Log",
                    "type":"text",
                    "file":"logs/.agave.log"
                },
                {
                    "displayName":"Mesh Error Log",
                    "type":"text",
                    "file":"logs/cwe-serial.err"
                },
                {
                    "displayName":"Mesh Output Log",
                    "type":"text",
                    "file":"logs/cwe-serial.out"
                }
            ]
        },
        "sim":{
            "displayName":"Simulation",
            "app":"cwe-parallel",
            "groups":[
                "Simulation Control",
                "Advanced"
            ],
            "results":[
                {
                    "displayName":"Final Flow Velocity Field",
                    "type":"GLdata",
                    "file":"U",
                    "values":"magnitude",
                    "showCondition":"XXXX"
                },
                {
                    "displayName":"Final Flow Pressure Field",
                    "type":"GLdata",
                    "file":"p",
                    "values":"scalar",
                    "showCondition":"XXXX"
                },
                {
                    "displayName":"Force Coefficients",
                    "type":"text",
                    "file":"postProcessing/forceCoeffs/0/forceCoeffs.dat"
                },
                {
                    "displayName":"Sim Agave Log",
                    "type":"text",
                    "file":"logs/.agave.log"
                },
                {
                    "displayName":"Sim Error Log",
                    "type":"text",
                    "file":"logs/cwe-parallel.err"
                },
                {
                    "displayName":"Sim Output Log",
                    "type":"text",
                    "file":"logs/cwe-parallel.out"
                },
                {
                    "displayName":"OpenFOAM Log",
                    "type":"text",
                    "file":"logs/sim.log"
                }
            ]
        }
    },
    "groups":{
        "Building Geometry":{
            "displayName":"Shape Geometry",
            "vars":[
                "shapeType",
                "dimensions",
                "itemD",
                "itemXwidth",
                "itemYwidth",
                "mirrored"
                ]
        },
        "Mesh Density":{
            "displayName":"Mesh Density",
            "vars":[
                "boundR",
                "boundMeshR",
                "boundMeshC",
                "boundNXf",
                "boundNXb",
                "boundNYt",
                "boundNYb",
                "boundRXf",
                "boundRXb",
                "boundMeshL",
                "boundMeshN",
                "boundRatio"

                ]
        },
        "Boundaries":{
            "displayName":"Boundaries",
            "vars":[
                "lowYPlane",
                "highYPlane"
            ]
        },

    cylinder
        


    rectangle


boundNXf = '35'
boundNXb = '35'
boundNYt = '10'
boundNYb = '10'
	
boundRXf = '2'
boundRXb = '2'

boundMeshL = '1.5'
boundMeshN = '25'
boundRatio = '1.5'

triangle

	
boundNXf = '35'
boundNXb = '35'
boundNYt = '10'
boundNYb = '10'

boundRXf = '2'
boundRXb = '2'

boundMeshL = '1.5'
boundMeshN = '25'
boundRatio = '1.5'



all
minX = '10'
minY = '10'
maxX = '40'
maxY = '10'
		
inletNum= '40'
outletNum= '120'
inletRatio= '5'
outletRatio= '15'
	
lowYNum= '40'
lowYratio= '5'
highYNum= '40'
highYratio= '5'
	



        "Simulation Control":{
            "displayName":"Simulation Control",
            "vars":[
                "endTime",
                "deltaT",
                "velocity",
                "nu"
            ]
        },
        "Advanced":{
            "displayName":"Advanced",
            "vars":[
                "turbModel",
                "useForceCoeffs",
                "useFieldUmean",
                "useFieldPmean",
                "pisoCorrectors",
                "pisoNonOrthCorrect",
                "turbintensity"
            ]
        }
    },
    "vars":{
        "shapeType":{
            "displayName":"Shape Type",
            "type":"choose",
            "default":"circle",
            "options":{
                "circle":"Circle",
                "triangle":"Triangle",
                "rectangle":"Rectangle"
            }
        },
        "dimensions":{
            "displayName":"Shape Type",
            "type":"choose",
            "default":"2",
            "options":{
                "2":"2D Simulation",
                "3":"3D Extrusion"
            }
        },
        "itemD":{
            "displayName":"Diameter of Circle",
            "type":"std",
            "default":"1.0",
            "unit":"m",
            "precision":"2",
            "sign":"+",
            "showCondition":"$shapeType=circle"
        },
        "itemXwidth":{
            "displayName":"Object Length (X)",
            "type":"std",
            "default":"3.0",
            "unit":"m",
            "precision":"2",
            "sign":"+",
            "showCondition":"$shapeType!circle"
        },
        "itemYwidth":{
            "displayName":"Object Length (Y)",
            "type":"std",
            "default":"1.0",
            "unit":"m",
            "precision":"2",
            "sign":"+",
            "showCondition":"$shapeType!circle"
        },
        "mirrored":{
            "displayName":"Reverse Triangle Orientation",
            "type":"bool",
            "default": false
        },
        "boundR":{
            "displayName":"Mesh Boundary Layer Radius",
            "type":"std",
            "default":"3.0",
            "unit":"m",
            "precision":"1",
            "sign":"+",
            "showCondition":"$shapeType=circle"
        },
        "boundR":{
            "displayName":"Mesh Boundary Layer Radius",
            "type":"std",
            "default":"60",
            "unit":"m",
            "precision":"0",
            "sign":"+",
            "showCondition":"$shapeType=circle"
        },



	
boundMeshR = '60'
boundMeshC = '200'
boundRatio = '50'


        "lowYPlane":{
            "displayName":"Boundary Condition (Bottom)",
            "type":"choose",
            "default":"SYM_PLANE",
            "options":{
                "SYM_PLANE": "Symmetry Plane",
                "WALL": "Wall"
            }
        },
        "highYPlane":{
            "displayName":"Boundary Condition (Top)",
            "type":"choose",
            "default":"SYM_PLANE",
            "options":{
                "SYM_PLANE": "Symmetry Plane",
                "WALL": "Wall"
            }
        },
        "endTime":{
            "displayName":"Simulation Duration",
            "type":"std",
            "default":"1.0",
            "unit":"s",
            "precision":"2",
            "sign":"+"
        },
        "deltaT":{
            "displayName":"Simulation Time Step",
            "type":"std",
            "default":"0.1",
            "unit":"s",
            "precision":"6",
            "sign":"+"
        },
        "velocity":{
            "displayName":"Inflow Velocity",
            "type":"std",
            "default":"1",
            "unit":"m/s",
            "precision":"6",
            "sign":"+"
        },
        "nu":{
            "displayName":"Kinematic Viscosity",
            "type":"std",
            "default":"1.48e-05",
            "unit":"m^2/s",
            "precision":"9",
            "sign":"+"
        },
        "useForceCoeffs":{
            "displayName":"Compute Force Coefficients?",
            "type":"bool",
            "default": true
        },
        "useFieldUmean":{
            "displayName":"Compute Mean Velocity Field?",
            "type":"bool",
            "default": false
        },
        "useFieldPmean":{
            "displayName":"Compute Mean Pressure Field?",
            "type":"bool",
            "default": false
        },
        "turbModel":{
            "displayName":"Turbulence Model",
            "type":"choose",
            "default":"laminar",
            "options":{
                "laminar":"Laminar Flow model",
                "SpalartAllmaras":"S-A one equation model (RANS)",
                "kEpsilon":"k-epsilon model (RANS)",
                "Smagorinsky":"Smagorinsky SGS model (LES)",
                "kEqn":"One equation eddy-viscosity model (LES)"
            }
        },
        "turbintensity":{
            "displayName":"Turbulence Intensity",
            "type":"std",
            "default":"0.1",
            "precision":"2",
            "sign":"+",
            "showCondition":"$turbModel!laminar"
        },
        "pisoCorrectors":{
            "displayName":"Number of PISO Correctors",
            "type":"std",
            "default":"2",
            "precision":"int",
            "sign":"+0"
        },
        "pisoNonOrthCorrect":{
            "displayName":"Number of non-orthogonal Correctors",
            "type":"std",
            "default":"1",
            "precision":"int",
            "sign":"+0"
        }
    }
}
